{
    "name": "nytris/ignition",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nytris\\Ignition\\": "src/"
        },
        "classmap": ["ignition.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Nytris\\Ignition\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "asmblah/php-code-shift": "^0.1.20",
        "mockery/mockery": "1.6.11",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-mockery": "^1.1",
        "phpunit/phpunit": "^10.2",
        "symfony/process": "^6.4"
    },
    "config": {
        "platform": {
            "php": "8.1"
        },
        "sort-packages": true
    }
}
